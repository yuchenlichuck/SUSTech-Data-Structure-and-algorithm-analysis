package speed;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double sp = sc.nextDouble();
		double v=sp/3.6;
		System.out.printf("<speed in %.2fkm/h> <b.d.="+"%.2f"+1.5*(v+0.5*v*v/0.7/9.8)+"m on wet road> <b.d="+"%.2f"+(v+0.5*v*v/0.7/9.8)+"m on dry road>");
		System.out.printf("<speed in %.2fkm/h>", sp);
		
	
	}
}
